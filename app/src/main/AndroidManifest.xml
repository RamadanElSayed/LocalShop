<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ramadanel_sayed.localshop">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyClasses.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/baske"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="AA_MODELS"
            android:value="com.activeandroid.Model" />

        <activity android:name=".splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".MainApp" />
        <activity android:name=".ActivityTerms" />
        <activity android:name=".AboutMe" />
        <activity
            android:name=".BuyActivity"
            android:label="@string/title_activity_buy"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SellActivity" />
        <activity android:name=".ProductProperties" />
        <activity android:name=".YourOrders"></activity>
    </application>

</manifest>